{
  "name": "react-helix-examples",
  "version": "1.0.0",
  "description": "Examples for react-helix library.",
  "scripts": {
    "clean": "rimraf app",
    "lint": "eslint src",
    "build": "npm-run-all clean lint build:mkapp build:server build:todo:html build:todo:js build:todo:server",
    "build:mkapp": "mkdirp app",
    "build:server": "babel src/server.js -o app/server.js",
    "build:todo:html": "cpx \"src/todo/client/**/*.html\" app/todo/client/",
    "build:todo:js": "browserify src/todo/client/index.js -t [envify --NODE_ENV=production] -t babelify | uglifyjs -cm > app/todo/client/index.js",
    "build:todo:server": "babel src/todo/server --out-dir app/todo/server",
    "start": "npm-run-all build start:server",
    "start:server": "node --harmony app/server.js",
    "dev": "npm-run-all build --parallel dev:server dev:todo:html dev:todo:js dev:todo:server dev:start",
    "dev:server": "npm run build:server -- --watch --source-maps-inline",
    "dev:todo:html": "npm run build:todo:html -- --watch",
    "dev:todo:js": "watchify src/todo/client/index.js -t [envify --NODE_ENV=development] -t babelify -o app/todo/client/index.js --verbose",
    "dev:todo:server": "npm run build:todo:server -- --watch --source-maps-inline",
    "dev:start": "node-dev --harmony ./app/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "immutable": "^3.7.1",
    "react": "^0.13.1",
    "react-helix": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.0.8",
    "babelify": "^6.0.1",
    "browserify": "^9.0.7",
    "cpx": "^1.1.5",
    "envify": "^3.4.0",
    "eslint": "^0.18.0",
    "eslint-plugin-react": "^2.0.2",
    "koa": "^0.19.0",
    "koa-bodyparser": "^1.5.0",
    "koa-mount": "^1.3.0",
    "koa-static": "^1.4.9",
    "mkdirp": "^0.5.0",
    "node-dev": "^2.6.1",
    "npm-run-all": "^1.1.2",
    "rimraf": "^2.3.2",
    "uglify-js": "^2.4.19",
    "watchify": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mysticatea/react-helix-examples.git"
  },
  "keywords": [
    "react",
    "flux",
    "react-helix",
    "example",
    "examples"
  ],
  "author": "Toru Nagashima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysticatea/react-helix-examples/issues"
  },
  "homepage": "https://github.com/mysticatea/react-helix-examples"
}
